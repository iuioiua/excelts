{
  "name": "@cj-tech-master/excelts",
  "version": "1.6.3",
  "description": "TypeScript Excel Workbook Manager - Read and Write xlsx and csv Files.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "cjnoname",
    "email": "powerfulj87@gmail.com"
  },
  "contributors": [
    {
      "name": "Guyon Roche",
      "email": "guyon@live.com",
      "url": "https://github.com/guyonroche"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cjnoname/excelts.git"
  },
  "homepage": "https://github.com/cjnoname/excelts#readme",
  "bugs": {
    "url": "https://github.com/cjnoname/excelts/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "browserslist": [
    "chrome >= 85",
    "edge >= 85",
    "firefox >= 79",
    "safari >= 14",
    "opera >= 71"
  ],
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/types/index.browser.d.ts",
        "default": "./dist/browser/excelts.esm.js"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "README_zh.md"
  ],
  "scripts": {
    "example": "node scripts/run-examples.ts",
    "check": "concurrently --raw \"npm run type\" \"npm run lint\"",
    "test": "vitest run && npm run test:browser",
    "test:browser": "rimraf dist && npm run build:browser && vitest run --config vitest.browser.config.ts",
    "test:watch": "vitest",
    "build": "rimraf dist && concurrently \"npm run build:esm\" \"npm run build:cjs\" \"npm run build:browser\"",
    "clean": "pnpm dlx --reporter=silent rimraf \"**/node_modules\" pnpm-lock.yaml \"**/dist\" -g && pnpm i",
    "lint": "eslint && oxlint",
    "lint:fix": "eslint --fix && oxlint --fix",
    "type": "tsgo --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "benchmark": "rimraf dist && npm run build:esm && node --expose-gc scripts/benchmark.js",
    "build:esm": "tsc --project tsconfig.esm.json && node scripts/fix-esm-imports.mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json && node scripts/fix-cjs-imports.mjs",
    "build:browser": "rolldown -c rolldown.config.js",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint && npm run type && npm run build && npm run test",
    "prepare": "husky"
  },
  "keywords": [
    "excelts",
    "typescript",
    "excel",
    "xlsx",
    "json",
    "csv",
    "spreadsheet",
    "workbook",
    "exceljs",
    "font",
    "border",
    "fill",
    "number",
    "format",
    "number format",
    "alignment",
    "office",
    "defined names",
    "data validations",
    "rich text",
    "in-cell format",
    "outlineLevel",
    "views",
    "frozen",
    "split",
    "pageSetup",
    "modern",
    "esm",
    "xlsx"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@typescript/native-preview": "^7.0.0-dev.20251225.1",
    "@vitest/browser": "^4.0.16",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-oxlint": "^1.35.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "oxlint": "^1.35.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "rolldown": "^1.0.0-beta.57",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  }
}
